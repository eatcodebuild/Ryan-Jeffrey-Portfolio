{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "rewrites": [
      {
        "source": "/bmi-calculator",
        "destination": "https://bmi-calculator-tau-wheat.vercel.app/"
      },

      {
        "source": "/coastal-joinery",
        "destination": "https://coastal-joinery.vercel.app/"
      },

      {
        "source": "/campout",
        "destination": "https://campout.vercel.app/"
      }
    ]
  }